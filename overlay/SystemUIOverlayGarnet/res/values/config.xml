<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Doze: does this device support STATE_DOZE?  -->
    <bool name="doze_display_state_supported">true</bool>

    <!-- Bottom margin size when fingerprint in display is active -->
    <dimen name="keyguard_indication_margin_bottom">6dp</dimen>

    <!-- Microns/ums (1000 um = 1mm) per pixel for the given device. If unspecified, UI that
         relies on this value will not be sized correctly. -->
    <item name="pixel_pitch" format="float" type="dimen">56.96</item>

    <!-- Location on the screen of the center of the physical power button. -->
    <dimen name="physical_power_button_center_screen_location_y">1223px</dimen>

     <!-- Preferred max refresh rate at AOD & Ambient Display. -->
     <integer name="config_aodMaxRefreshRate">30</integer>
  
    <!-- Burn in protection -->
    <bool name="config_statusBarBurnInProtection">true</bool>
    <integer name="config_shift_interval">60</integer>
	
</resources>
